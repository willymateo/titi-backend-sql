{
  "name": "titi-backend-sql",
  "version": "1.2.0",
  "private": true,
  "description": "Backend API RestFul SQL to Titi web",
  "main": "src/index.js",
  "author": "DarkOS",
  "keywords": [
    "api",
    "restful",
    "sql",
    "titi"
  ],
  "scripts": {
    "build": "babel src -d build",
    "start": "sequelize db:migrate && npm run build && node build/index.js",
    "start:dev": "NODE_ENV=development sequelize db:migrate && nodemon src/index.js --exec babel-node",
    "test": "NODE_ENV=test sequelize db:migrate && PORT=4001 jest",
    "prettier:write": "prettier --write . --ignore-unknown",
    "prettier:check": "prettier --check .",
    "migrate": "sequelize db:migrate",
    "migrate:undo:all": "sequelize db:migrate:undo:all",
    "prepare": "husky install",
    "eslint": "eslint ."
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "express": "~4.18.2",
    "express-fileupload": "^1.4.0",
    "express-list-routes": "^1.1.3",
    "helmet": "^6.0.1",
    "jsonwebtoken": "^9.0.0",
    "morgan": "~1.10.0",
    "pg": "^8.9.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.28.0",
    "serve-favicon": "^2.5.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "@babel/register": "^7.18.9",
    "eslint": "^8.33.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.6.0",
    "husky": "^8.0.3",
    "jest": "^29.4.1",
    "lint-staged": "^13.1.0",
    "nodemon": "^2.0.20",
    "prettier": "2.8.3",
    "sequelize-cli": "^6.6.0",
    "supertest": "^6.3.3"
  }
}
