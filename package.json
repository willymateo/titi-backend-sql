{
  "name": "cathot-backend-sql",
  "version": "0.0.1",
  "private": true,
  "description": "Backend API RestFul SQL to CatHot web",
  "main": "src/index.js",
  "author": "DarkOS",
  "keywords": [
    "api",
    "restful",
    "sql",
    "cathot"
  ],
  "scripts": {
    "build": "babel src -d build",
    "start": "sequelize db:migrate && npm run build && NODE_ENV=production node build/index.js",
    "start:dev": "NODE_ENV=development nodemon src/index.js --exec babel-node",
    "start:test": "NODE_ENV=test node src/index.js --exec babel-node",
    "test": "NODE_ENV=test PORT=4001 jest",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write . --ignore-unknown",
    "migrate": "sequelize db:migrate",
    "prepare": "husky install"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.0.0",
    "express": "~4.16.1",
    "express-list-routes": "^1.1.3",
    "helmet": "^5.1.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.17.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.2",
    "@babel/node": "^7.17.10",
    "@babel/preset-env": "^7.18.2",
    "@babel/register": "^7.17.7",
    "eslint": "^8.13.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.7",
    "nodemon": "^2.0.15",
    "prettier": "2.6.2",
    "sequelize-cli": "^6.4.1",
    "supertest": "^6.2.2"
  }
}
